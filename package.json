{
  "name": "format-fuse.js",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Utility to format matching fuse.js results for easier text highlighting",
  "author": "Eric Liu (https://github.com/metonym)",
  "main": "lib/format-fuse.js.js",
  "types": "lib/format-fuse.js.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:tdd": "jest --watch",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "set-value": "^3.0.1",
    "strind": "^0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.20",
    "@types/set-value": "^2.0.0",
    "fuse.js": "^3.4.5",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint --fix -p .",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/format-fuse.js.git"
  },
  "homepage": "https://github.com/metonym/format-fuse.js",
  "keywords": [
    "format",
    "fuse.js",
    "highlight",
    "matches"
  ],
  "files": [
    "lib"
  ],
  "bugs": "https://github.com/metonym/format-fuse.js/issues"
}
